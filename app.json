{
  "expo": {
    "name": "tutti-frutti-game",
    "slug": "tutti-frutti-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#AE13FF"
    },
    "primaryColor" : "#B93CF4",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jdev.tuttiFruttiGame",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jdev.tuttiFruttiGame",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#AE13FF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
